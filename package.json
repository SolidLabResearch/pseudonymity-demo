{
  "name": "evaluation",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "docker:run": "docker compose -f docker-compose.yml up -d",
    "docker:stop": "docker compose stop -t 0",
    "docker:rm": "docker compose rm --force",
    "docker:logs": "docker compose logs -f",
    "docker:clean-start": "npm run docker:stop && npm run docker:rm && npm run docker:run",
    "index": "npx ts-node src/index.ts",
    "index:watch": "nodemon --config nodemon.json --exec npm run index",
    "setup": "npx ts-node src/setup.ts",
    "setup:watch": "nodemon --config nodemon.json --exec npm run setup",
    "d2:setup:watch": "d2 --watch ./diagrams/setup.d2 ./diagrams/setup.png",
    "d2:sequence:3rdparty:watch": "d2 --watch ./diagrams/sequence-3rd-party.d2 ./diagrams/sequence-3rd-party.png",
    "d2:sequence:did:watch": "d2 --watch ./diagrams/sequence-did-key.d2 ./diagrams/sequence-did-key.png",
    "d2:trust:watch": "d2 --watch ./diagrams/trust.d2 ./diagrams/trust.png",
    "d2:credential:watch": "d2 --watch ./diagrams/credential-exchange.d2 ./diagrams/credential-exchange.png",
    "eval:hacky:recruiter:start": "npx ts-node src/components/recruiter/service.ts",
    "eval:hacky:recruiter:watch": "nodemon --config nodemon.json -w ./src/components/recruiter --exec npm run eval:hacky:recruiter:start",
    "eval:hacky:anonymizer:start": "npx ts-node src/components/anonymizer/service.ts",
    "eval:hacky:anonymizer:watch": "nodemon --config nodemon.json -w ./src/components/anonymizer --exec npm run eval:hacky:anonymizer:start",
    "test:eval:hacky:recruiter": "npx ts-node src/tests/components/recruiter-service.ts",
    "test:eval:hacky:recruiter:watch": "nodemon --config nodemon.json --exec npm run test:eval:hacky:recruiter",

    "sandbox": "npx ts-node sandbox.ts",
    "sandbox:watch": "nodemon --config nodemon.json -w sandbox.ts --exec npm run sandbox"
  },
  "author": "GDM",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/n3": "^1.16.3",
    "@types/node": "^20.8.7",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@comunica/query-sparql": "^2.10.0",
    "@digitalbazaar/did-method-key": "^5.1.0",
    "@digitalbazaar/vc": "^6.0.2",
    "@inrupt/solid-client": "^1.30.2",
    "@inrupt/solid-client-authn-core": "^1.17.3",
    "@mattrglobal/jsonld-signatures-bbs": "^1.2.0",
    "body-parser": "^1.20.2",
    "credentials-context": "^2.0.0",
    "cross-fetch": "^4.0.0",
    "did-context": "^3.1.1",
    "express": "^4.18.2",
    "file-type": "^18.5.0",
    "jsonld-signatures": "^11.2.1",
    "mime-types": "^2.1.35",
    "n3": "^1.17.1",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3"
  }
}
