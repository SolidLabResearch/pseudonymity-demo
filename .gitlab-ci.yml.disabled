stages:
  - test

image: docker:latest

services:
  - docker:dind


before_script:
  - docker info
  - apk update
  - apk upgrade
  - apk add curl
  - apk add --no-cache --upgrade bash
  - apk add python3 py3-pip build-base
  - pip install docker-compose
  - sh -c bash
  - apk add --update nodejs npm
  - apk add --update openjdk11
  - apk add openssl
  - npm install

test-utils:
  stage: test

  script:
    - npm run test:utils


#after_script:
#    - npm run docker:test:stop


